
# http://editorconfig.org
root = true

# Global settings for every file
[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_size = 4
indent_style = tab

[*.xml]
indent_size = 4
indent_style = tab

[*.txt]
indent_size = 4
indent_style = tab
